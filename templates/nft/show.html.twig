{% extends 'base.html.twig' %}

{% block title %}NFT
{% endblock %}

{% block body %}
	<h1>NFT</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ nft.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ nft.name }}</td>
			</tr>
			<tr>
				<th>Img</th>
				<td>{{ nft.img }}</td>
			</tr>
			<tr>
				<th>ExistingNumber</th>
				<td>{{ nft.existingNumber }}</td>
			</tr>
			<tr>
				<th>LaunchDate</th>
				<td>{{ nft.launchDate ? nft.launchDate|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>LaunchPriceEth</th>
				<td>{{ nft.launchPriceEth }}</td>
			</tr>
			<tr>
				<th>LaunchPriceEur</th>
				<td>{{ nft.launchPriceEur }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_nft_index') }}">back to list</a>

	<a href="{{ path('app_nft_edit', {'id': nft.id}) }}">edit</a>

	{{ include('nft/_delete_form.html.twig') }}
{% endblock %}
